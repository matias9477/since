{
  "expo": {
    "name": "Since",
    "slug": "since",
    "version": "1.2.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#fdfea0"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.matiasturra.since",
      "buildNumber": "3",
      "infoPlist": {
        "NSUserNotificationUsageDescription": "This app uses notifications to congratulate you about your time sensitive accomplishments.",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#fdfea0"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "NOTIFICATIONS",
        "NOTIFICATIONS"
      ],
      "package": "com.matiasturra.since"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "bc18fb25-dfa4-49d9-821b-699264586dc2"
      }
    },
    "owner": "matias.turra",
    "plugins": [
      "expo-font",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#fdfea0"
        }
      ]
    ]
  }
}
